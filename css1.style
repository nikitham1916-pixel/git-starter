body {
  font-family: Arial, sans-serif;
  margin: 0;
  padding: 0;
  line-height: 1.6;
  background-color: #f4f4f4;
}

header, footer {
  background-color: #333;
  color: white;
  padding: 1rem;
  text-align: center;
}

main {
  padding: 2rem;
}

section {
  margin-bottom: 2rem;
}
.services {
  list-style-type: square;
  padding-left: 2rem;
  color: #333;
  font-size: 1rem;
}

.services li {
  margin-bottom: 0.5rem;
}

